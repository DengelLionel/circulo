    align-items: center;
    opacity: 0;
    pointer-events: none;
    background: #000;
    transition: opacity 350ms;
  }
  .section-video .video-container.video-container--modal-mobile .video-container__stop {
    display: block;
    position: fixed;
    top: 0;
    left: 0;
    color: #fff;
    z-index: 102;
    padding: 19px;
    pointer-events: none;
    opacity: 0;
    transition: opacity 350ms;
